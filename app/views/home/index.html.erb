<div class="page-title">
  <div class="container-fluid">
    <h1><%= page_meta.title %></h1>
  </div>
</div>
<div class="main-top">
  <div class="main-top__info">
    <h1>International  Ticket Agency</h1>
    <div class="main-top__form">
      <%= form_tag events_path, method: :get do  %>
        <%= text_field_tag :query, params[:query], :placeholder => "Search for an artist, team or city", id: 'main-top-form-query' %>
        <%= submit_tag '' %>
      <% end %>
    </div>
  </div>
  <div class="main-top__slider">
    <% if @top_slides.any? %>
      <% @top_slides.each do |slide| %>
      <div class="main-top__item" style="background: url(<%= slide.image.url %>)">
        <div class="container-fluid">
          <div class="main-top__events">
            <div class="main-top__events-inner">
              <div class="main-top__events-title"><%= slide.view_title %></div>
              <div class="main-top__events-place"><%= slide.view_place %></div>
              <div class="event-date">
                <% if slide.view_start_date.present? %>
                  <%= render partial: 'shared/event_date_item', locals: { the_date: slide.view_start_date } %>
                <% end %>
                <% if slide.view_end_date.present? %>
                  <%= render partial: 'shared/event_date_item', locals: { the_date: slide.view_end_date } %>
                <% end %>
              </div>
              <%= link_to 'Show Event', slide.manual_input? ? slide.url : event_path(slide.event), :class => 'btn' %>
            </div>
          </div>
        </div>
      </div>
      <% end %>
    <% else %>
      <div class="main-top__item" style="background: url('<%= asset_path('top_slider_default_back.jpg') %>')"></div>
    <% end %>
  </div>
</div>
