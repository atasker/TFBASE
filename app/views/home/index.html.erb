<% if @featured_slides.any? %>
<div class="main-featured">
  <div class="container-fluid">
    <h2>Featured Events</h2>
    <div class="main-featured__slider">
      <% @featured_slides.each do |slide| %>
        <div class="main-featured__slide">
          <div class="main-featured__inner">
            <% slide_url = slide.manual_input? ? slide.url : event_path(slide.event) %>
            <%= link_to '', slide_url, :class => 'main-featured__img', style: "background-image:url(#{slide.image.url})" %>
            <%= link_to slide.view_title, slide_url, :class => 'main-featured__title' %>
            <% if slide.view_category %>
              <div class="main-featured__labels">
                <%= link_to slide.view_category.description, category_path(slide.view_category), :class => "main-featured__label green--sqheme" %>
              </div>
            <% end %>
            <span class="main-featured__place"><%= slide.view_place %></span>
            <% if slide.view_start_date.present? || slide.view_end_date.present? %>
              <div class="event-date">
                <% if slide.view_start_date.present? %>
                  <%= render partial: 'shared/event_date_item', locals: { the_date: slide.view_start_date } %>
                <% end %>
                <% if slide.view_end_date.present? %>
                  <%= render partial: 'shared/event_date_item', locals: { the_date: slide.view_end_date } %>
                <% end %>
              </div>
            <% end %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
<% end %>
<% if @top_events.any? %>
<div class="main-top-events">
  <div class="container-fluid">
    <h2>Top events</h2>
    <div class="row">
      <% toppest_event = @top_events.first %>
      <div class="col-md-6 main-top-event__col">
        <div class="main-top-event main-top-event--full-h">
          <%= link_to (toppest_event.manual_input? ? toppest_event.url : event_path(toppest_event.event)), :class => 'main-top-event__inner', style: "background-image:url(#{toppest_event.big_image.the_first.url})" do %>
            <% if toppest_event.view_start_date.present? || toppest_event.view_end_date.present? %>
              <div class="event-date">
                <% if toppest_event.view_start_date.present? %>
                  <%= render partial: 'shared/event_date_item', locals: { the_date: toppest_event.view_start_date } %>
                <% end %>
                <% if toppest_event.view_end_date.present? %>
                  <%= render partial: 'shared/event_date_item', locals: { the_date: toppest_event.view_end_date } %>
                <% end %>
              </div>
            <% end %>
            <div class="main-top-event__info">
              <span><%= toppest_event.view_title %></span>
              <% if toppest_event.view_category %>
                <div class="main-top-event__label"><%= toppest_event.view_category.description %></div>
              <% end %>
              <div class="main-top-event__place"><%= toppest_event.view_place %></div>
            </div>
          <% end %>
        </div>
      </div>
      <div class="col-md-6 main-top-event__col">
        <% @top_events[1..3].each do |slide| %>
          <div class="col-md-12 main-top-event main-top-event--small-h">
            <%= link_to (slide.manual_input? ? slide.url : event_path(slide.event)), :class => 'main-top-event__inner', style: "background-image:url(#{slide.big_image.other.url})" do %>
              <% if slide.view_start_date.present? || slide.view_end_date.present? %>
                <div class="event-date">
                  <% if slide.view_start_date.present? %>
                    <%= render partial: 'shared/event_date_item', locals: { the_date: slide.view_start_date } %>
                  <% end %>
                  <% if slide.view_end_date.present? %>
                    <%= render partial: 'shared/event_date_item', locals: { the_date: slide.view_end_date } %>
                  <% end %>
                </div>
              <% end %>
              <div class="main-top-event__info">
                <span><%= slide.view_title %></span>
                <% if slide.view_category %>
                  <div class="main-top-event__label"><%= slide.view_category.description %></div>
                <% end %>
                <div class="main-top-event__place"><%= slide.view_place %></div>
              </div>
            <% end %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
<% end %>
